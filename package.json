{
  "name": "proxy-verifier",
  "version": "0.4.7",
  "description": "Check that proxies are working, verify their anonymity level, lookup their geographic location by IP address, check for other capabilities such as tunneling and available protocols.",
  "keywords": [
    "proxy",
    "proxies",
    "verify",
    "check",
    "anonymity"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --config .eslintrc.js *.js test/**/*.js",
    "prepublish": "npm run test",
    "pretest": "npm run lint",
    "test": "npm run test:unit",
    "test:unit": "./node_modules/.bin/mocha test/unit/ --recursive --reporter spec --ui bdd"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/seankovacs/proxy-verifier"
  },
  "author": "Sean Kovacs",
  "license": "MIT",
  "readmeFilename": "readme.md",
  "dependencies": {
    "async": "3.2.0",
    "depd": "2.0.0",
    "geoip-lite": "1.4.2",
    "proxy-agent": "3.1.1",
    "request": "2.88.2",
    "underscore": "1.10.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "7.5.0",
    "express": "4.17.1",
    "http-proxy": "1.18.1",
    "mocha": "8.0.1"
  }
}
